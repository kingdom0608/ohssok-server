{
  "name": "ohssok-server",
  "version": "1.1.0",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "build:event-server": "nest build event-server",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:local:api-server": "NODE_ENV=local nest start api-server --watch",
    "start:develop:api-server": "NODE_ENV=develop node dist/apps/api-server/main",
    "start:production:api-server": "NODE_ENV=production node dist/apps/api-server/main",
    "start:local:event-server": "NODE_ENV=local nest start event-server --watch",
    "start:develop:event-server": "NODE_ENV=develop node dist/apps/event-server/main",
    "start:production:event-server": "NODE_ENV=production node dist/apps/event-server/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/api-server/test/jest-e2e.json"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.425.0",
    "@aws-sdk/client-sqs": "^3.441.0",
    "@aws-sdk/s3-request-presigner": "^3.289.0",
    "@faker-js/faker": "^7.6.0",
    "@nestjs/bull": "^10.0.1",
    "@nestjs/common": "^9.3.1",
    "@nestjs/config": "^2.3.0",
    "@nestjs/core": "^9.3.1",
    "@nestjs/platform-express": "^9.4.2",
    "@nestjs/swagger": "^6.1.4",
    "@nestjs/typeorm": "^9.0.1",
    "@ssut/nestjs-sqs": "^2.1.0",
    "@types/multer": "^1.4.7",
    "axios": "^1.3.2",
    "bull": "^4.10.4",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cookie-parser": "^1.4.6",
    "crypto-js": "^4.1.1",
    "date-fns": "^2.30.0",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "mysql": "^2.18.1",
    "pbkdf2": "^3.1.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "solapi": "^5.2.3",
    "swagger-themes": "^1.2.27",
    "typeorm": "^0.3.11",
    "uuidv4": "^6.2.13",
    "voucher-code-generator": "^1.3.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/aws-lambda": "^8.10.117",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.13",
    "@types/jest": "29.2.4",
    "@types/lodash": "^4.14.202",
    "@types/node": "18.11.18",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "29.3.1",
    "plugin": "^0.0.15",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "29.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.1",
    "typescript": "^4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/libs/",
      "<rootDir>/apps/"
    ],
    "moduleNameMapper": {
      "^@app/common(|/.*)$": "<rootDir>/libs/common/src/$1",
      "^@app/authentication(|/.*)$": "<rootDir>/libs/authentication/src/$1"
    }
  }
}
